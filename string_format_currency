//Format a string of numbers to display a currency - example "1234.678" to "1,234.68".
#include<stdio.h>
#include<string.h>
#include<stdlib.h>
int main()
{
  int i,count=0,temp=0,l=0;
  char arr[100];
  scanf("%s",arr);
  for(i=0;arr[i]!='.';i++){
     count++;}

  if(count<=3){
    printf("%s",arr);
    exit(1);
  }
  else{
      i=0;
      if(count%2==0)
      {
          printf("%c,",arr[i]);
          for(i=1;i<count-3;i+=2){
                printf("%c%c,",arr[i],arr[i+1]);
           }
      }
      if(count%2!=0)
      {
          printf("%c%c,",arr[i],arr[i+1]);
          for(i=2;i<count-3;i+=2){
                printf("%c%c,",arr[i],arr[i+1]);
            }
      }
    }
    if(count==4){
        count=1;}
    else{
        count-=3;
    }
    for(i=count;i<strlen(arr)-1;i++)
    {
        printf("%c",arr[i]);
    }

}
