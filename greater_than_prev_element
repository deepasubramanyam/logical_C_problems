#include <stdio.h>
int main() {
   int arr[100],i,count=1,n,k;
   scanf("%d",&n);
   for(i=0;i<n;i++)
   {
       scanf("%d",&arr[i]);
   }
   printf("%d",arr[0]);
   k=arr[0];
   for(i=1;i<n;i++)
   {
       if(k<arr[i])
       {
           k=arr[i];
           printf("%d",arr[i]);
       }
   }
  /* for(i=1;i<n;i++)
