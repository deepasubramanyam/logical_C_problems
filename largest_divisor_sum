#include<stdio.h>
void main()
{
    int T,i,j,a;
    long int sum=0,k,n;
    scanf("%d",&T);
    for(a=0;a<T;a++){
        sum=0;
        scanf("%ld%ld",&n,&k);
        for(i=1;i<=n;i++)
        {
            for(j=i;j>=1;j--){
                if(i%j==0 &&j%k!=0)
                {
                    sum+=j;
                    break;
                }
            }
        }
        printf("%ld",sum);
   }
}
