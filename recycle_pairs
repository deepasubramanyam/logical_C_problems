input:10
123
98765
12
34
8
9
76598
3412
43
3
output:2
#include<stdio.h>
#include<math.h>
void main()
{
    int arr[100000],i,n,j,t,count=0,pairs=0,result=0,t1=0,k=0;
    scanf("%d",&n);
    for(i=0;i<n;i++){
        scanf("%d",&arr[i]);
    }
    for(i=0;i<n;i++)
    {
        for(j=0;j<n;j++)
        {
            if(arr[i]<arr[j] &&arr[i]!=arr[j])
            {
                t=arr[i];
                while(t!=0){
                t=t/10;
                count++;}
            }
            t1=arr[i];
            while(k<count){
               result=(t1%10)*pow(10,count-1)+t1/10;
               t1=result;
               if(count>=3){
                t1++;}
                if(t1==arr[j]){
                    pairs++;
                    break;
                }
            k++;}
            result=0;
            k=0;
            count=0;
      }
   }
   printf("%d",pairs);
}
